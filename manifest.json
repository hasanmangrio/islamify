{
    "name": "Islamify New Tab",
    "version": "1.1",
    "manifest_version": 3,
    "description": "Custom New Tab theme for Muslims",
    "chrome_url_overrides": {
        "newtab": "islamify.html"
    },
    "permissions": [
        "geolocation", "storage"
    ],
    "content_security_policy": {
        "extension_pages": "default-src 'self'; connect-src http://ipinfo.io http://www.islamicfinder.us/index.php/api/;"
    }
}